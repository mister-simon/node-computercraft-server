local actionsDict = {
	{
		movement = turtle.forward,
		detect = turtle.detect,
		attack = turtle.attack,
		dig = turtle.dig
	},
	{
		movement = turtle.up,
		detect = turtle.detectUp,
		attack = turtle.attackUp,
		dig = turtle.digUp
	},
	{
		movement = turtle.down,
		detect = turtle.detectDown,
		attack = turtle.attackDown,
		dig = turtle.digDown
	}
}

-- Params: Function
function getActionsListFromMovementAction(movement)
	for  i = 1, #actionsDict do
		if movement == actionsDict[i].movement then
			return actionsDict[i]
		end
	end
end